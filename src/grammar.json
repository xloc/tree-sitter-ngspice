{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "ngspice",
  "rules": {
    "source_file": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "title_line"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "instance_line"
              },
              {
                "type": "SYMBOL",
                "name": "control_line"
              }
            ]
          }
        }
      ]
    },
    "title_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "line_content"
        },
        {
          "type": "SYMBOL",
          "name": "newline"
        }
      ]
    },
    "instance_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "resistor_instance"
            },
            {
              "type": "SYMBOL",
              "name": "capacitor_instance"
            },
            {
              "type": "SYMBOL",
              "name": "inductor_instance"
            },
            {
              "type": "SYMBOL",
              "name": "transistor_instance"
            },
            {
              "type": "SYMBOL",
              "name": "voltage_source_instance"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "newline"
        }
      ]
    },
    "resistor_instance": {
      "type": "SEQ",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "PATTERN",
              "value": "r|R"
            },
            {
              "type": "IMMEDIATE_TOKEN",
              "content": {
                "type": "PATTERN",
                "value": "[0-9a-zA-Z]*"
              }
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "node"
        },
        {
          "type": "SYMBOL",
          "name": "node"
        },
        {
          "type": "SYMBOL",
          "name": "value"
        }
      ]
    },
    "capacitor_instance": {
      "type": "SEQ",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "PATTERN",
              "value": "c|C"
            },
            {
              "type": "IMMEDIATE_TOKEN",
              "content": {
                "type": "PATTERN",
                "value": "[0-9a-zA-Z]*"
              }
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "node"
        },
        {
          "type": "SYMBOL",
          "name": "node"
        },
        {
          "type": "SYMBOL",
          "name": "value"
        }
      ]
    },
    "inductor_instance": {
      "type": "SEQ",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "PATTERN",
              "value": "l|L"
            },
            {
              "type": "IMMEDIATE_TOKEN",
              "content": {
                "type": "PATTERN",
                "value": "[0-9a-zA-Z]*"
              }
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "node"
        },
        {
          "type": "SYMBOL",
          "name": "node"
        },
        {
          "type": "SYMBOL",
          "name": "value"
        }
      ]
    },
    "transistor_instance": {
      "type": "SEQ",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "PATTERN",
              "value": "q|Q"
            },
            {
              "type": "IMMEDIATE_TOKEN",
              "content": {
                "type": "PATTERN",
                "value": "[0-9a-zA-Z]*"
              }
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "node"
        },
        {
          "type": "SYMBOL",
          "name": "node"
        },
        {
          "type": "SYMBOL",
          "name": "node"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        }
      ]
    },
    "voltage_source_instance": {
      "type": "SEQ",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "PATTERN",
              "value": "v|V"
            },
            {
              "type": "IMMEDIATE_TOKEN",
              "content": {
                "type": "PATTERN",
                "value": "[0-9a-zA-Z]*"
              }
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "node"
        },
        {
          "type": "SYMBOL",
          "name": "node"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "dc"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "ac"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "time_dependent_value"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "dc": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "PATTERN",
              "value": "dc|DC"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "value"
        }
      ]
    },
    "ac": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "ac|AC"
        },
        {
          "type": "SYMBOL",
          "name": "value"
        }
      ]
    },
    "time_dependent_value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "PATTERN",
              "value": "sin|SIN"
            },
            {
              "type": "STRING",
              "value": "("
            },
            {
              "type": "SYMBOL",
              "name": "value"
            },
            {
              "type": "SYMBOL",
              "name": "value"
            },
            {
              "type": "SYMBOL",
              "name": "value"
            },
            {
              "type": "STRING",
              "value": ")"
            }
          ]
        }
      ]
    },
    "control_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": ".model"
            },
            {
              "type": "STRING",
              "value": ".tran"
            },
            {
              "type": "STRING",
              "value": ".end"
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "value"
              },
              {
                "type": "SYMBOL",
                "name": "identifier"
              },
              {
                "type": "SEQ",
                "members": [
                  {
                    "type": "STRING",
                    "value": "("
                  },
                  {
                    "type": "REPEAT",
                    "content": {
                      "type": "SYMBOL",
                      "name": "assignment"
                    }
                  },
                  {
                    "type": "STRING",
                    "value": ")"
                  }
                ]
              }
            ]
          }
        }
      ]
    },
    "assignment": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "SYMBOL",
          "name": "value"
        }
      ]
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z][a-zA-Z0-9]*"
    },
    "node": {
      "type": "PATTERN",
      "value": "[a-zA-Z0-9]+"
    },
    "value": {
      "type": "PATTERN",
      "value": "[0-9.]+(e|E-?\\d+)?[a-zA-Z]*"
    },
    "line_content": {
      "type": "PATTERN",
      "value": ".*"
    },
    "newline": {
      "type": "PATTERN",
      "value": "\\r?\\n"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s+"
    },
    {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "\n+"
          },
          {
            "type": "PATTERN",
            "value": "\\s*"
          }
        ]
      }
    },
    {
      "type": "PATTERN",
      "value": ";.*"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
